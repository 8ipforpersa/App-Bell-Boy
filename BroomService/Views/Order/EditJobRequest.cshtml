        }



    #div1 {
        width: 700px;
        height: 300px;
        padding: 10px;
        border: 1px solid #aaaaaa;
    }

    .draggable {
        filter: alpha(opacity=100);
        opacity: 1.0;
    }

    .dropped {
        position: static !important;
        z-index: 100;
    }

    #dragIcons {
        padding: 5px;
        min-height: 120px;
        width: 500px;
    }

    #briefcase {
        border: 5px solid #ccc;
        height: 300px;
